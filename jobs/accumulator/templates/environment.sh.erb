<%
  nozzles = link("nozzle")
  nozzle_addrs = nozzles.instances.map do |i|
    "http://#{i.address}:#{nozzles.p("http.port")}"
  end
%>
export UAA_ADDR="<%= nozzles.p("uaa.addr") %>"
export CLIENT_ID="<%= nozzles.p("uaa.client_id") %>"
export CLIENT_SECRET="<%= nozzles.p("uaa.client_secret") %>"
export NOZZLE_ADDRS="<%= nozzle_addrs.join(",") %>"
export DATADOG_API_KEY="<%= p("datadog.api_key") %>"
export SKIP_CERT_VERIFY="<%= p("tls.skip_cert_verify") %>"
export REPORTER_HOST="<%= p("reporter.host") %>"
